<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <WebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

    <ImageButton
        android:id="@+id/btninfo"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="100dp"
        android:background="@drawable/bottomrad"
        android:minWidth="48dp"
        android:minHeight="48dp"
        app:circularflow_radiusInDP="15dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/iconoleyenda"
        tools:ignore="SpeakableTextPresentCheck" />

    <ImageButton
        android:id="@+id/btninfocontaminantes"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginEnd="25dp"
        android:layout_marginBottom="24dp"
        android:background="@drawable/bottomrad2"
        android:minWidth="38dp"
        android:minHeight="38dp"
        app:circularflow_radiusInDP="15dp"
        app:layout_constraintBottom_toTopOf="@+id/btninfo"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/informacion"
        tools:ignore="SpeakableTextPresentCheck" />

</androidx.constraintlayout.widget.ConstraintLayout>